;PRGOGRAM  WRITE ARRAY SYMBOL10 CONVERT OCT
	MOV #1070,R4 ;WRITE NUMB8
	MOV #ARR1,R5 ;WRITE ADR 0 ARR1
	MOV #ARR2,R2 ;WRITE ADR 0 ARR2
	MOV #ARR2,ADR ;WRITE ADRESS ARR2 TO ADR
	MOV #47.,R3	 ;WRITE CODE SYMBOL 0-9	
1:	CMP (R5)+,R4 ;COMPARE R5-R0
	BHI 1 		 ;IF C=0 AND Z=0
2:	DEC R5 	   	 ;CORRECT ADR ARR1
	DEC R5		 ;TODO?
3:	SUB (R5),R4  ;NUMBER - MANTIS
	INC R3		 ;COUNT CODE SYMBOL 0 1 2 . .9	
	BHIS 3		 ;IF C>=0 GOTO 3:
	ADD (R5)+,R4 ;CORRECT NUMB8
	MOV R3,(R2)+ ;WRITE RESULT SYMBOL TO ARR2
	MOV #47.,R3	 ;CORRECT CODE SYMBOL 0-9
	MOV (R5),R0
	BEQ 4		 ;Z=1
	BNE 3		 ;Z=0	
4:	HALT
ARR1: .WORD 1000.,100.,10.,1.,0 ;MANTIS
ARR2: .WORD 0,0,0,0,0,0 ;ARR CODE NUMBER DEC FOR 3 MANTIS (ПОСЛЕДНИЙ БАЙТ 0 ДЛЯ ОБОЗНАЧЕНИЯ КОНЦА СТРОКИ)
ADR:  .WORD 0 ;ADR ARR2
;4:  RTS	